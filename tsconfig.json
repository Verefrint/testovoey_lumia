{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./",
    "moduleResolution": "node",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./typechain-types/*"]
    }
  },
  "include": ["./scripts/**/*", "./test/**/*", "./typechain-types/**/*"],
  "exclude": ["node_modules"]
}